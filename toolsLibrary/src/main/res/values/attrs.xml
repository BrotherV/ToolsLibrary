<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WidgetAttributes" >
        <attr name="tl_backgroundColor" format="reference"/>
        <attr name="tl_strokeColor" format="reference"/>
        <attr name="tl_strokeSize" format="dimension"/>
        <attr name="tl_cornerRadius" format="dimension"/>
        <attr name="tl_shapeType" format="integer">
            <enum name="rectangle" value="0xA01" />
            <enum name="oval" value="0xA02" />
        </attr>
        <attr name="tl_dropDownIcon" format="integer">
            <enum name="simple" value="0xA03" />
            <enum name="cornerIcon" value="0xA04" />
        </attr>
        <attr name="tl_layoutDirection" format="integer">
            <enum name="ltr" value="1" />
            <enum name="rtl" value="2" />
        </attr>
        <attr name="tl_iconColor" format="reference"/>
        <attr name="tl_textColor" format="reference"/>
        <attr name="tl_textSize" format="dimension"/>
        <attr name="tl_imageSize" format="dimension"/>
        <attr name="tl_entries" format="reference" />
        <attr name="tl_entryValues" format="reference" />
        <attr name="tl_imageEntries" format="reference" />
        <attr name="tl_rtlTypeFace" format="string"/>
        <attr name="tl_ltrTypeFace" format="string"/>
        <attr name="tl_fontType" format="reference"/>

        <attr name="tl_rippleColor" format="color" localization="suggested" />
        <attr name="tl_rippleDimension" format="dimension" localization="suggested" />
        <attr name="tl_rippleOverlay" format="boolean" localization="suggested" />
        <attr name="tl_rippleAlpha" format="float" localization="suggested" />
        <attr name="tl_rippleDuration" format="integer" localization="suggested" />
        <attr name="tl_hoverDuration" format="integer" localization="suggested" />
        <attr name="tl_rippleFadeDuration" format="integer" localization="suggested" />
        <attr name="tl_rippleHover" format="boolean" localization="suggested" />
        <attr name="tl_rippleBackground" format="color" localization="suggested" />
        <attr name="tl_rippleDelayClick" format="boolean" localization="suggested" />
        <attr name="tl_ripplePersistent" format="boolean" localization="suggested" />
        <attr name="tl_rippleInAdapter" format="boolean" localization="suggested" />
        <attr name="tl_rippleStartFromCenter" format="boolean" localization="suggested" />
        <attr name="tl_rippleFixCircleRadius" format="boolean" localization="suggested" />
        <attr name="tl_rippleEnableMove" format="boolean" localization="suggested" />
        <attr name="tl_rippleRoundedCorners" format="dimension" localization="suggested" />

        <attr name="tl_cardCollapsedHeight" format="dimension"/>
        <attr name="tl_labelColor" format="color"/>
        <attr name="tl_image" format="reference"/>
        <attr name="tl_hasFocus" format="boolean"/>
        <attr name="tl_animationDuration" format="integer"/>
        <attr name="tl_openKeyboardOnFocus" format="boolean"/>

        <attr name="cpb_progress" format="integer" />
        <attr name="cpb_progress_max" format="integer" />
        <attr name="cpb_indeterminate_mode" format="boolean" />
        <attr name="cpb_progressbar_color" format="color" />
        <attr name="cpb_progressbar_color_start" format="color" />
        <attr name="cpb_progressbar_color_end" format="color" />
        <attr name="cpb_progressbar_color_direction">
            <flag name="left_to_right" value="1" />
            <flag name="right_to_left" value="2" />
            <flag name="top_to_bottom" value="3" />
            <flag name="bottom_to_top" value="4" />
        </attr>
        <attr name="cpb_background_progressbar_color" format="color" />
        <attr name="cpb_background_progressbar_color_start" format="color" />
        <attr name="cpb_background_progressbar_color_end" format="color" />
        <attr name="cpb_background_progressbar_color_direction">
            <flag name="left_to_right" value="1" />
            <flag name="right_to_left" value="2" />
            <flag name="top_to_bottom" value="3" />
            <flag name="bottom_to_top" value="4" />
        </attr>
        <attr name="cpb_progressbar_width" format="dimension" />
        <attr name="cpb_background_progressbar_width" format="dimension" />
        <attr name="cpb_round_border" format="boolean" />
        <attr name="cpb_start_angle" format="float" />
        <attr name="cpb_progress_direction">
            <flag name="to_right" value="1" />
            <flag name="to_left" value="2" />
        </attr>

        <attr name="tl_animatorDurationSec" format="integer" />
        <attr name="tl_paddingRectangleFromEdge" format="dimension" />
        <attr name="tl_widthRec" format="dimension" />
        <attr name="tl_heightRec" format="dimension" />
        <attr name="tl_roundCornerInDp" format="dimension" />
        <attr name="tl_strokeWidthPath" format="dimension" />
        <attr name="tl_pathDirection" format="integer" />
        <attr name="tl_colorPath" format="color"/>
        <attr name="tl_pathDirectionType">
            <flag name="up_to_down" value="1" />
            <flag name="down_to_up" value="0" />
        </attr>
        <attr name="tl_waveViewStyle" format="reference"/>

        <attr name="wv_shapeType" format="enum">
            <enum name="circle" value="1" />
            <enum name="square" value="2" />
            <enum name="heart" value="3" />
        </attr>
        <attr name="wv_progress" format="integer" />
        <attr name="wv_max" format="integer" />
        <attr name="wv_borderWidthSize" format="dimension" />
        <attr name="wv_frontColor" format="color" />
        <attr name="wv_backColor" format="color" />
        <attr name="wv_borderColor" format="color" />
        <attr name="wv_strong" format="integer" />
        <attr name="wv_textColor" format="color" />
        <attr name="wv_animatorEnable" format="boolean" />
        <attr name="wv_textHidden" format="boolean" />
        <attr name="wv_shapePadding" format="dimension" />

        <attr name="pb_progress" format="integer" />
        <attr name="pb_max" format="integer" />
        <attr name="pb_frontColor" format="color" />
        <attr name="pb_backColor" format="color" />

    </declare-styleable>

    <declare-styleable name="FillableLoader">
        <attr name="fl_fillColor"
            format="color"/>
        <attr name="fl_strokeColor"
            format="color"/>
        <attr name="fl_strokeWidth"
            format="dimension"/>
        <attr name="fl_originalWidth"
            format="integer"/>
        <attr name="fl_originalHeight"
            format="integer"/>
        <attr name="fl_strokeDrawingDuration"
            format="integer"/>
        <attr name="fl_fillDuration"
            format="integer"/>
        <attr name="fl_fillPercentage"
            format="integer"/>
        <attr name="fl_clippingTransform"
            format="enum">
            <enum name="plain"
                value="0"/>
            <enum name="spikes"
                value="1"/>
            <enum name="rounded"
                value="2"/>
            <enum name="waves"
                value="3"/>
            <enum name="squares"
                value="4"/>
            <enum name="bites"
                value="5"/>
        </attr>
    </declare-styleable>
</resources>